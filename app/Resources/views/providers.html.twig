<div class="provider-div-report">
    <div class="dropdown select-provider">
        <button type="button" class="btn btn-default dropdown-toggle" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">{{ provider_name }}<span class="caret"></span></button>
        <ul class="dropdown-menu">
            {% for provider in providers %}
                <li><a class="dropdown-item" href="/report?provider={{ provider.identifier }}">{{ provider.name }}</a></li>
            {% endfor %}
        </ul>
    </div>
</div>
