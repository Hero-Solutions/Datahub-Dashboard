<div class="provider-div-report">
    <div class="dropdown select-provider">
        <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-expanded="false">
            {{ provider_name }}
        </button>
        <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton">
            {% for provider in providers %}
                <li><a class="dropdown-item" href="/{{ app.request.locale }}/report/{{ provider.identifier }}">{{ provider.name }}</a></li>
            {% endfor %}
        </ul>
    </div>
</div>
